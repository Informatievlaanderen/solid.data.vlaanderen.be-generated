    {
      "EA-Guid": "{07458847-C860-4fc7-9FC2-FAF13A74A701}",
      "name": "Europees Digitaal Credential",
      "label": {
        "nl": "Europees Digitaal Credential",
        "fr": "Enter your translation here"
      },
      "definition": {
        "nl": "Een reeks beweringen die door een uitgever in Europa worden gemaakt, gebruikmakend van Europese standaarden. Een Europees Digitaal Credential is een reeks van een of meer beweringen die kunnen worden gebruikt om aan te tonen dat de eigenaar bepaalde vaardigheden bezit of bepaalde leeruitkomsten heeft bereikt door middel van formeel, niet-formeel of informeel leren.",
        "fr": "Enter your translation here"
      }
    },
    {
      "EA-Guid": "{10307864-0B53-4fc3-9735-3F9EE0C73AD5}",
      "name": "Leerprestatie",
      "label": {
        "nl": "Leerprestatie",
        "fr": "Enter your translation here"
      },
      "definition": {
        "nl": "De verwerving van kennis, vaardigheden of verantwoordelijkheid en zelfstandigheid. Een erkend en/of toegekend geheel van leeruitkomsten van een individu.",
        "fr": "Enter your translation here"
      }
    },
    {
      "EA-Guid": "{5E31E613-A24E-4185-AC7C-07E8C904CB26}",
      "name": "Leerprestatiespecificatie",
      "label": {
        "nl": "Leerprestatiespecificatie",
        "fr": "Enter your translation here"
      },
      "definition": {
        "nl": "Een beschrijving van wat een persoon kan leren door gebruik te maken van de leeropportuniteit, uitgedrukt als leeruitkomsten. Een specificatie van leerprestaties.",
        "fr": "Enter your translation here"
      }
    },
    {
      "EA-Guid": "{DA1E6F12-A633-4e92-9479-84C375E3094E}",
